using System;

namespace ConsoleApp39
{
     class Employess
    {
        public string name;
        public int age;
        public int id;
        public void Employedetail (string name, int age, int id)
        {
            this.name=name;
            this.age=age;
            this.id=id;

            Console.WriteLine("name:{0} age:{1} Id:{2}",this.name,this.age,this.id);
        }
    }

    class Program
    {
        static void Main(string[] args)
        {
            Employess a = new Employess();
            a.Employedetail("naman", 12, 30000);



        }
    }
}
